{
  "name": "test-driven-carousel",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint:js": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0 && prettier --list-different .",
    "lint:css": "stylelint \"**/*.{ts,tsx}\"",
    "lint": "npm run lint:js && npm run lint:css",
    "format:js": "eslint --fix . && prettier --loglevel warn --write .",
    "format:css": "stylelint \"**/*.{ts,tsx}\" --fix",
    "format": "npm run format:js && npm run format:css",
    "test": "vitest",
    "preview": "vite preview"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.0.1",
    "@testing-library/react": "^14.3.1",
    "@testing-library/user-event": "^14.4.3",
    "@types/eslint": "^8.56.0",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@types/styled-components": "^5.1.34",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitejs/plugin-react": "^4.0.3",
    "babel-plugin-styled-components": "^2.1.4",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "eslint-plugin-testing-library": "^6.0.1",
    "happy-dom": "^9.20.3",
    "jest-styled-components": "^7.1.1",
    "postcss-styled-syntax": "^0.4.0",
    "prettier": "^2.8.8",
    "styled-components": "^6.0.9",
    "stylelint": "^15.10.2",
    "stylelint-config-standard": "^34.0.0",
    "typescript": "^5.3.3",
    "vite": "^4.4.5",
    "vitest": "^0.34.6"
  }
}
